
        <div class="container-xl">
            <div class="table-responsive">
                <div class="table-wrapper">
                    <div class="table-title">
                        <div class="row">
                              <input class="form-control" id="myInput" type="text" placeholder="Search..">
                        </div>
                        <div class="row">
                            <div class="col-sm-6">
                                {{#if message}}<div class="alert alert-info"><strong>System: </strong> {{message}}</div>{{/if}}             
                                {{#if error}}<div class="alert alert-danger"><strong>Error:</strong> {{error}}</div>{{/if}}
                                {{#if errors}}<div class="alert alert-danger"><strong>Error:</strong> {{errors}}</div>{{/if}}
                                <h2><b>Total Active Booked Room: {{active_list.length}}</b></h2>
                            </div>
                            <div class="col-sm-6">
                                <a href="/bookedroom/add" class="btn btn-success" ><i class="material-icons">&#xE147;</i> <span>Book New Room</span></a>
                            </div>
                        </div>
                    </div>
                    <table class="table table-striped table-hover" border="1" cellpadding="10" cellspacing="0" style="width: 100%; table-layout: fixed;">
                        <thead>
                            <tr>
                                <th style="width: 5%;">#</th>
                                <th style="width: 30%;">Room</th>
                                <th style="width: 20%;">Day</th>
                                <th style="width: 15%;">Time Start</th>
                                <th style="width: 15%;">Time End</th>
                                <th style="width: 30%;">Action</th>
                            </tr>
                        </thead>
                        <tbody id="myTable">		
                            {{#each active_list}}
                                <tr>
                                    <td>{{inc @index}}</td>
                                    <td>{{ room.name }}</td>
                                    <td >{{ day }}</td>
                                    <td>{{ time_start }}</td>
                                    <td>{{time_end}}</td>
                                    <td>
                                        <a href="/bookedroom/edit/{{_id}}" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
                                        <a href="/bookedroom/delete/{{_id}}" class="delete" onclick="return confirm('Do you want to delete this room: {{room.name}} ?' )" ><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
                                        <a href="/bookedroom/detail/{{_id}}" class="btn btn-outline-success" role="button">Detail</a>
                                        <a href="/bookedroom/deactive/{{_id}}" class="btn btn-outline-success" role="button">Deactive</a>
                                    </td>
                                </tr> 
                            {{/each}}
                        </tbody>
                    </table>
                </div>

                <div class="table-wrapper">
                    <div class="table-title">
                        <div class="row">
                            <div class="col-sm-6">
                                <h2><b>Total Deactive Booked Room: {{deactive_list.length}}</b></h2>
                            </div>
                        </div>
                    </div>
                    <table class="table table-striped table-hover" border="1" cellpadding="10" cellspacing="0" style="width: 100%; table-layout: fixed;">
                        <thead>
                            <tr>
                                <th style="width: 5%;">#</th>
                                <th style="width: 30%;">Room</th>
                                <th style="width: 20%;">Day</th>
                                <th style="width: 15%;">Time Start</th>
                                <th style="width: 15%;">Time End</th>
                                <th style="width: 30%;">Action</th>
                            </tr>
                        </thead>
                        <tbody id="myTable">		
                            {{#each deactive_list}}
                                <tr>
                                    <td>{{inc @index}}</td>
                                    <td>{{ room.name }}</td>
                                    <td >{{ day }}</td>
                                    <td>{{ time_start }}</td>
                                    <td>{{time_end}}</td>
                                    <td>
                                        <a href="/bookedroom/edit/{{_id}}" class="edit" ><i class="material-icons" data-toggle="tooltip" title="Edit">&#xE254;</i></a>
                                        <a href="/bookedroom/delete/{{_id}}" class="delete" onclick="return confirm('Do you want to delete this room: {{room.name}} ?' )" ><i class="material-icons" data-toggle="tooltip" title="Delete">&#xE872;</i></a>
                                        <a href="/bookedroom/detail/{{_id}}" class="btn btn-outline-success" role="button">Detail</a>
                                        <a href="/bookedroom/Active/{{_id}}" class="btn btn-outline-success" role="button">Active</a>
                                    </td>
                                </tr> 
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>        
        </div>